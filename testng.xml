<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
<listeners>
<listener class-name="com.RightsfuAlly.Utilities.ExtentListenerClass"></listener>
<listener class-name="com.RightsfuAlly.Utilities.AnnotationTransformer"></listener>
</listeners>
  <test name="Test">
    <classes>
      <class name="com.RightsfuAlly.TestCases.SignInViaEmailWithMobileVerification">
      <methods>
      <exclude name="signInViaEmailWithMobileVerificationByNewOrVerifiedEmailAndExistingContact"></exclude>
      <exclude name="signInViaEmailWithMobileVerificationByNewOrVerifiedEmailAndAssociatedVerifiedContact"></exclude>
      <exclude name="signInViaEmailByExistingEmail"></exclude>
      <exclude name="signInViaEmailByAssociatedVerifiedEmail"></exclude>
      <exclude name="signInViaEmailWithMobileVerificationByNewOrVerifiedEmailAndNewContact"></exclude>
      <exclude name="signInViaEmailWithMobileVerificationByNewOrVerifiedEmailAndVerifiedContact"></exclude>
      <exclude name="signInViaEmailWithMobileVerificationByNewOrVerifiedEmailAndAssociatedUnverifiedContact"></exclude>
      <exclude name="signInViaEmailByAssociatedUnverifiedEmail"></exclude>
      </methods>
      </class>
      
      <class name="com.RightsfuAlly.TestCases.SignInViaEmailWithMobileVerificationLater">
      <methods>
      <exclude name="signInViaEmailWithMobileVerificationLaterByNewOrVerifiedEmailAndVerifiedContact"></exclude>
      <exclude name="signInViaEmailWithMobileVerificationLaterByNewOrVerifiedEmailAndAssociatedUnverifiedContact"></exclude>
      <exclude name="signInViaEmailWithMobileVerificationLaterByNewOrVerifiedEmailAndExistingContact"></exclude>
      <exclude name="signInViaEmailWithMobileVerificationLaterByNewOrVerifiedEmailAndAssociatedVerifiedContact"></exclude>
      <exclude name="signInViaEmailWithMobileVerificationLaterByNewOrVerifiedEmailAndNewContact"></exclude>
      </methods>
      </class>
      
      <class name="com.RightsfuAlly.TestCases.SignInViaSmsWithEmailVerification">
      <methods>
      <exclude name="signInViaSmsWithEmailVerificationByNewOrVerifiedContactAndNewEmail"></exclude>
      <exclude name="signInViaSmsWithEmailVerificationByNewOrVerifiedContactAndVerifiedEmail"></exclude>
      <exclude name="signInViaSmsWithEmailVerificationByNewOrVerifiedContactAndAssociatedUnverifiedEmail"></exclude>
      <exclude name="signInViaSmsWithEmailVerificationByNewOrVerifiedContactAndExistingEmail"></exclude>
      <exclude name="signInViaSmsWithEmailVerificationByNewOrVerifiedContactAndAssociatedVerifiedEmail"></exclude>
      <include name="signInViaSmsByExistingContact"></include>
      <exclude name="signInViaSmsByAssociatedVerifiedContact"></exclude>
      <exclude name="signInViaSmsByAssociatedUnverifiedContact"></exclude> 
      </methods>
      </class>
      
      <class name="com.RightsfuAlly.TestCases.SignInViaSmsWithEmailVerificationLater">
      <methods>
      <exclude name="signInViaSMSWithEmailVerificationLaterByNewOrVerifiedContactAndNewEmail"></exclude>
      <exclude name="signInViaSmsWithEmailVerificationLaterByNewOrVerifiedContactAndVerifiedEmail"></exclude>
      <exclude name="signInViaSmsWithEmailVerificationLaterByNewOrVerifiedContactAndAssociatedUnverifiedEmail"></exclude>
      <exclude name="signInViaSmsWithEmailVerificationLaterByNewOrVerifiedContactAndExistingEmail"></exclude>
      <exclude name="signInViaSmsWithEmailVerificationLaterByNewOrVerifiedContactAndAssociatedVerifiedEmail"></exclude>
      </methods>
      </class>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
